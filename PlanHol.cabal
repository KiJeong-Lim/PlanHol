name: PlanHol
version: 0.1.0
synopsis: A Programming Language with Higher-Order Logic
build-type: Simple
cabal-version: 2.0

executable main
  hs-source-dirs: src
  default-language: Haskell2010
  main-is: Main.hs
  ghc-options: -O3
  build-depends: base, containers, transformers, pretty-terminal
  other-modules:
    Hol.Front.Ast
    Hol.Front.Grammar
    Hol.Main
    LGS.Main
    PGS.Main
    Z.Utils
