name: PlanHol
version: 0.1.0
synopsis: a Programming LANguage with Higher-Order Logic
build-type: Simple
cabal-version: 2.0

executable PlanHol
  hs-source-dirs: src
  default-language: Haskell2010
  main-is: Main.hs
  ghc-options: -O3
  build-depends: base >= 4.7 && < 5, containers >= 0.6, transformers >= 0.5
  other-modules:
    Hol.BEnd.Unique
